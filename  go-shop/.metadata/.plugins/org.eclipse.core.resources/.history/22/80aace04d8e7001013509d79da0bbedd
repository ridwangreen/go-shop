package GUI;

import java.awt.Color;
import java.awt.Image;
import java.io.File;
import java.io.IOException;

import javax.imageio.ImageIO;
import javax.swing.ImageIcon;

public class PegColor extends ImageIcon{
	
	public PegColor(Color color, int size) throws IOException{
		
		if(size == 50){
			if(color.equals(Color.GRAY)){
				super.setImage(ImageIO.read(new File("rcs/pegs/disabled.png")));
			}
		}
	}
}
